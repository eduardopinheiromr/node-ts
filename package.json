{
  "name": "node-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts",
    "db:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "db:seed": "knex --knexfile knexfile.ts seed:run",
    "db:restart": "rm -rf src/database/database.sqlite && yarn db:migrate && yarn db:seed"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.95.7",
    "multer": "^1.4.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.2",
    "nodemon": "^2.0.12",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
